{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  },
  "type":"CommonJS",
  "include": ["src/**/*","src/*","./*.ts"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}